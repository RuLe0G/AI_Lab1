<Window x:Class="AI_Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI_Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Загрузить файлы" Command="{Binding LoadFilesCommand}" Width="200" Height="100" Margin="10,10,590,324" />
        <ListBox ItemsSource="{Binding ListBoxItems}" SelectedItem="{Binding SelectedItem, Mode=TwoWay}" HorizontalAlignment="Center" Height="293" VerticalAlignment="Top" Width="780" Margin="0,131,0,0"/>
    </Grid>
</Window>
